generator client {
  provider = "prisma-client-js"
}
 
datasource db {
  provider = "postgresql"
  url = env("POSTGRES_PRISMA_URL") // uses connection pooling
  directUrl = env("POSTGRES_URL_NON_POOLING") // uses a direct connection
}

model tabela {
  id          Int     @id @default(autoincrement())
  products_id Int     @unique
  datetime    DateTime
  @@map("tabela")
}

model products {
  id          Int     @id @default(autoincrement())
  products_name_id Int     @unique
  box_id      Int
  quantity_box Int
  quantity_item Int
  @@map("products")
}

model products_name {
  id          Int     @id @default(autoincrement())
  name        String
  @@map("products_name")
}

model stock {
  id          Int     @id @default(autoincrement())
  box_id      Int
  products_id Int
  shelfs_sections_id Int
  @@map("stock")
}

model shelfs_sections {
  id          Int     @id @default(autoincrement())
  shelf       String
  section     Int
  @@map("shelfs_sections")
}

model remove {
  id          Int     @id @default(autoincrement())
  products_id Int
  box_id      Int
  shelfs_sections_id Int
  quantity_box Int
  datetime    DateTime
  @@map("remove")
}
